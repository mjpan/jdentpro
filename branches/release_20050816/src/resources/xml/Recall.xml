<?xml version="1.0" encoding="UTF-8"?>

<panel id="panel" layout="GridBagLayout">


<panel>
 <gridbagconstraints id="gbc" gridx="1" gridy="0" weightx="1" weighty="1" anchor="GridBagConstraints.LINE_START"/>
 <label Font="Helvetica 36" Foreground="blue" text="Recall"/>
</panel>

<label LabelFor="recallTypeComboBox1" font="Helvetica" foreground="blue" text="Type">
 <gridbagconstraints id="gbc1" gridx="0" gridy="2" weightx="1" weighty="1" anchor="GridBagConstraints.LINE_END"/>
</label>
<combobox id="recallTypeComboBox1" PreferredSize="200,22">
 <gridbagconstraints id="gbc2" gridx="1" gridy="2" weightx="1" weighty="1" anchor="GridBagConstraints.LINE_START"/>
</combobox>
 
<label LabelFor="recallTypeComboBox2" font="Helvetica" foreground="blue" text="Type">
 <gridbagconstraints use="gbc1" gridx="2" gridy="2"/>
</label>
<combobox id="recallTypeComboBox2" PreferredSize="200,22">
 <gridbagconstraints use="gbc2" gridx="3" gridy="2"/>
</combobox>

<label LabelFor="recallTypeComboBox3" font="Helvetica" foreground="blue" text="Type">
 <gridbagconstraints use="gbc1" gridx="4" gridy="2"/>
</label>
<combobox id="recallTypeComboBox3" PreferredSize="200,22">
 <gridbagconstraints use="gbc2" gridx="5" gridy="2"/>
</combobox>

